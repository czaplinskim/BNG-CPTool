{
  "name": "Newest FPC Finder",
  "description": "Chrome Extension that will look for the newest FPC and save your time.",
  "version": "0.1",
  "manifest_version": 3,
  "icons": {
      "16": "./icons/icon_16.png",
      "32": "./icons/icon_32.png",
      "48": "./icons/icon_48.png",
      "128": "./icons/icon_128.png"
  },
  "web_accessible_resources": [{
    "matches": ["<all_urls>"],
    "resources": ["*"]
  }],
  "content_scripts": [
    {
      "js": ["content-script.js"],
      "matches": [
        "https://pg.browser.synthrone.io/*",
        "https://pg.tabler.synthrone.io/*"
      ]
    }
  ]

}

